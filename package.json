{
  "name": "media-center-frontend",
  "version": "1.0.0",
  "dependencies": {
    "angular": "1.5.6",
    "angular-ui-router": "0.3.1",
    "es6-promise": "3.2.1",
    "font-awesome": "4.6.3",
    "jquery": "3.0.0",
    "lodash": "4.13.1"
  },
  "devDependencies": {
    "angular-mocks": "1.5.7",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "http-server": "0.9.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chai-sinon": "0.1.5",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-junit-reporter": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sinon": "1.0.5",
    "karma-source-map-support": "1.1.0",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript-preprocessor2": "1.2.1",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "node-sass": "3.7.0",
    "phantomjs-prebuilt": "2.1.7",
    "remap-istanbul": "0.6.4",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "tslint": "3.14.0",
    "typescript": "1.8.10",
    "typings": "1.3.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1"
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/typings install",
    "start": "npm run build:dev",
    "clean": "rm -rf tmp && rm -rf dist && rm -rf test/results",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json --type-check -c tslint.json {app,test}/**/*.ts",
    "build": "./node_modules/.bin/webpack",
    "build:dev": "./node_modules/.bin/webpack -w",
    "build:min": "./node_modules/.bin/webpack -p",
    "test": "npm run clean && npm run test:unit && npm run test:cov",
    "test:unit": "./node_modules/.bin/karma start test/config/karma.unit.conf.js",
    "test:cov": "./node_modules/.bin/karma start test/config/karma.cov.conf.js && npm run istanbul:remap",
    "test:func": "echo 'No functional test yet'",
    "istanbul:remap": "npm run istanbul:remap:html && npm run istanbul:remap:lcov",
    "istanbul:remap:html": "./node_modules/.bin/remap-istanbul -i test/results/coverage/karma-coverage/json/coverage.json -o test/results/coverage/istanbul-remap/html -t html -e test",
    "istanbul:remap:lcov": "./node_modules/.bin/remap-istanbul -i test/results/coverage/karma-coverage/json/coverage.json -o test/results/coverage/istanbul-remap/lcov/lcov.info -t lcovonly -e test",
    "serve": "./node_modules/.bin/http-server . -p 9000"
  }
}
